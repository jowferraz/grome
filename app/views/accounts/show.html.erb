<div class="page-header">
  <%= link_to accounts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_account_path(@account), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show account</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @account.name %></dd>

  <dt>Cpf:</dt>
  <dd><%= @account.cpf %></dd>

  <dt>Rg:</dt>
  <dd><%= @account.rg %></dd>

  <dt>Street:</dt>
  <dd><%= @account.street %></dd>

  <dt>Number:</dt>
  <dd><%= @account.number %></dd>

  <dt>Zipcode:</dt>
  <dd><%= @account.zipcode %></dd>

  <dt>Country:</dt>
  <dd><%= @account.country %></dd>

  <dt>Accept terms:</dt>
  <dd><%= @account.accept_terms %></dd>

  <dt>User:</dt>
  <dd><%= @account.user_id %></dd>

</dl>
